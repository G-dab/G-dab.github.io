@font-face{font-family:harmonyos_sans_bold;src:url(/fonts/HarmonyOS_Sans_Bold.ttf)format("truetype")}@font-face{font-family:robotoslab-regular;src:url(/fonts/RobotoSlab-Regular.ttf)format("truetype")}.profile-container{display:flex;justify-content:space-between;align-items:center;padding:1% 10%}.profile-intro{font-family:RobotoSlab-Regular,sans-serif;flex:1;padding-right:10%}.profile-image img{width:100%;max-width:300px;height:auto;border-radius:2px}.home-heading{padding:1% 10%}button{background-color:#fff;border:3px solid;border-radius:12px;color:#000;padding:15px 32px;text-align:center;text-decoration:none;font-family:HarmonyOS_Sans_Bold,sans-serif;display:inline-block;font-size:16px;margin:4px 2px;cursor:pointer;transition-duration:.4s;-webkit-transition-duration:.4s}button:hover{box-shadow:0 12px 16px rgba(0,0,0,.24),0 17px 50px rgba(0,0,0,.19)}button:active{background-color:#2b4420;box-shadow:0 5px #666;transform:translateY(4px)}